@model ZipLocation
@using ZipMap.Model;

@{
    ViewData["Title"] = "Zip";
}

<div id="map" style="width:100%;height:500px"></div>

<script>
    function myMap() {
        var myCenter = new google.maps.LatLng(39.997216699934654, -83.0424830318043);
        var mapCanvas = document.getElementById("map");
        var mapOptions = { center: myCenter, zoom: 5 };
        var map = new google.maps.Map(mapCanvas, mapOptions);
        var marker = new google.maps.Marker({ position: myCenter });
        marker.setMap(map);
    }
</script>

<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD5bDAqZWnnoAdLoe6j8LvLDlnVlT0ndUU&callback=myMap"></script>

